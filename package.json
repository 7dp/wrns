{
  "name": "Starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:qa": "react-native run-android --mode=qaDebug",
    "android:qa:release": "react-native run-android --mode=qaRelease",
    "android:prod": "react-native run-android --mode=productionDebug",
    "android:prod:release": "react-native run-android --mode=productionRelease",
    "build:qa": "cd android; ENVFILE=.env.qa ./gradlew assembleQaRelease; cd ..",
    "build:prod": "cd android; ENVFILE=.env.production ./gradlew assembleProductionRelease; cd ..",
    "build:aab": "cd android; ENVFILE=.env.production ./gradlew bundleProductionRelease; cd ..",
    "ios:qa": "ENVFILE=.env.qa react-native run-ios --scheme 'StarterQA'",
    "ios:prod": "ENVFILE=.env.production react-native run-ios --scheme 'Starter'",
    "bundle-ios": "npx react-native bundle --entry-file ./index.js --platform ios --bundle-output ios/main.jsbundle --assets-dest ios",
    "clean": "cd android; ./gradlew clean; cd ..",
    "ios": "react-native run-ios",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "postinstall": "husky install && patch-package",
    "prepare": "husky install",
    "start": "react-native start",
    "test": "jest"
  },
  "lint-staged": {
    "src/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "index.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.5.1",
    "@notifee/react-native": "^7.8.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-firebase/app": "^17.4.3",
    "@react-native-firebase/crashlytics": "^17.4.3",
    "@react-native-firebase/messaging": "^17.4.3",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "@reduxjs/toolkit": "^1.9.7",
    "@shopify/flash-list": "^1.6.1",
    "async-mutex": "^0.4.0",
    "dayjs": "^1.11.10",
    "expo": "^50.0.0-canary-20231205-250b31f",
    "expo-image": "0.0.1-canary-20231205-250b31f",
    "expo-keep-awake": "0.0.1-canary-20231205-250b31f",
    "expo-localization": "0.0.1-canary-20231205-250b31f",
    "expo-status-bar": "0.0.1-canary-20231205-250b31f",
    "i18n-js": "^4.3.2",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-native": "0.73.1",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-keyboard-controller": "^1.8.0",
    "react-native-mmkv": "^2.10.2",
    "react-native-modal-dropdown": "^1.0.2",
    "react-native-network-logger": "^1.15.0",
    "react-native-pressable-scale": "^2.1.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "4.7.4",
    "react-native-screens": "~3.27.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@tsconfig/react-native": "^3.0.0",
    "@types/i18n-js": "^3.8.6",
    "@types/react": "^18.2.6",
    "@types/react-native-modal-dropdown": "^1.0.2",
    "@types/react-native-vector-icons": "^6.4.15",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "lint-staged": "^14.0.1",
    "metro-react-native-babel-preset": "0.76.8",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "pretty-quick": "^3.1.3",
    "react-native-flipper": "^0.212.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.2",
    "typescript": "^5.1.3"
  },
  "prettier": {
    "bracketSameLine": false,
    "bracketSpacing": true,
    "printWidth": 90,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=18"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-application",
        "expo-constants",
        "expo-file-system",
        "expo-font"
      ]
    }
  }
}
